{
  "name": "convert2mermaid",
  "version": "1.0.0",
  "description": "Convert diagrams from other formats into mermaidJs syntax",
  "license": "MIT",
  "type": "module",
  "author": "Justin Greywolf <jgreywolf@lostware.com>",
  "scripts": {
